{% extends "base/base.tpl" %}
{% block title %}
    Advisor
{% endblock %}
{% set active_no = '' %}
{% set active_subnav_no = '' %}
{% block content %}

<div class="l-inner">
    <div class="l-advisor">
        <ul class="c-advisor-list">
            <li class="c-advisor-list__item">
                <div class="c-advisor c-advisor--alt">
                    <h4 class="c-advisor__heading">
                        Banker
                    </h4>
                    <p class="c-advisor__company ">
                        Company Name
                    </p>
                    <address class="c-advisor__address">
                        <p class="t-subtitle-1 t-light">Address line 2</p>
                        <p class="t-subtitle-1 t-light">Address line 3</p>
                        <p class="t-subtitle-1 t-light">Address line 4</p>
                    </address>
                    <div class="c-advisor__action">
                        <a href="#" class="c-underline-link">
                            Find out more
                            <span class="c-underline-link__icon o-icon-wrapper">
                                <svg class="o-icon o-icon--arrow-right">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="./static/svg/symbol/svg/sprite.symbol.svg#icon-arrow-right"></use>
                                </svg>
                            </span>
                        </a>
                    </div>
                </div>
            </li>
            <li class="c-advisor-list__item">
